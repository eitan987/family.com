{
  "name": "family-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=9000 next dev --port 9000",
    "dev:local": "cross-env NODE_ENV=development PORT=8080 next dev --port 8080 --hostname localhost",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "reset": "rm -rf .next node_modules package-lock.json && npm install && npm run dev:local",
    "check-ports": "lsof -i :3000,8080,9000",
    "server": "node src/server/server.js",
    "dev:server": "nodemon src/server/server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "deploy:vercel": "vercel",
    "deploy:vercel:prod": "vercel --prod",
    "deploy:firebase": "firebase deploy",
    "deploy:firebase:hosting": "firebase deploy --only hosting",
    "deploy:firebase:setup": "firebase login && firebase init"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "next": "14.0.0",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-calendar": "^4.6.1",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^5.5.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "firebase-tools": "^13.5.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vercel": "^33.3.0"
  }
}
